<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:navigationtest="http://schemas.android.com/apk/res/com.demo.navigationtest"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <com.demo.navigationtest.ui.user.health_info.UserInfoCardComponent
        android:layout_width="match_parent"
        android:layout_height="80dp"
        navigationtest:user_info_card_component_content="img"
        navigationtest:user_info_card_component_title="头像" />

    <com.demo.navigationtest.ui.user.health_info.UserInfoCardComponent
        android:layout_width="match_parent"
        android:layout_height="80dp"
        android:id="@+id/user_info_phone"
        navigationtest:user_info_card_component_content=""
        navigationtest:user_info_card_component_title="手机号码" />

    <com.demo.navigationtest.ui.user.health_info.UserInfoCardComponent
        android:layout_width="match_parent"
        android:layout_height="80dp"
        android:id="@+id/user_info_studentId"
        navigationtest:user_info_card_component_content=""
        navigationtest:user_info_card_component_title="学号" />

    <com.demo.navigationtest.ui.user.health_info.UserInfoCardComponent
        android:layout_width="match_parent"
        android:layout_height="80dp"
        android:id="@+id/user_info_name"
        navigationtest:user_info_card_component_content=""
        navigationtest:user_info_card_component_title="姓名" />

    <com.demo.navigationtest.ui.user.health_info.UserInfoCardComponent
        android:layout_width="match_parent"
        android:layout_height="80dp"
        android:id="@+id/user_info_gender"
        navigationtest:user_info_card_component_content=""
        navigationtest:user_info_card_component_title="性别" />




</LinearLayout>